
from tkinter import *

root = Tk()
root.tittle =("Login Usuario")

mainFrame = Frame(root)
mainFrame.pack()
mainFrame.config(width=480,height=320,bg="lightbblue")

titulo = Label(mainFrame,text="Login De Usuairo De ElectricWorld ",front=("Arial",24))
titulo.grid(column=0,row=0,padx=10,pady=10)#,columnspan=0)




root.mainloop()

